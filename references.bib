@article{simunovic_when_2015,
	title = {When {Physics} {Takes} {Over}: {BAR} {Proteins} and {Membrane} {Curvature}},
	volume = {25},
	issn = {0962-8924},
	url = {http://dx.doi.org/10.1016/j.tcb.2015.09.005},
	doi = {10.1016/j.tcb.2015.09.005},
	abstract = {Cell membranes become highly curved during membrane trafficking,
cytokinesis, infection, immune response, or cell motion.
Bin/amphiphysin/Rvs (BAR) domain proteins with their intrinsically curved
and anisotropic shape are involved in many of these processes, but with a
large spectrum of modes of action. In vitro experiments and multiscale
computer simulations have contributed in identifying a minimal set of
physical parameters, namely protein density on the membrane, membrane
tension, and membrane shape, that control how bound BAR domain proteins
behave on the membrane. In this review, we summarize the multifaceted
coupling of BAR proteins to membrane mechanics and propose a simple phase
diagram that recapitulates the effects of these parameters.},

@article{astumian_thermodynamics_1997,
	title = {Thermodynamics and kinetics of a {Brownian} motor},
	volume = {276},
	issn = {0036-8075},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/9139648},
	abstract = {Nonequilibrium fluctuations, whether generated externally or by a chemical
reaction far from equilibrium, can bias the Brownian motion of a particle

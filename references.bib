@article{boettner_f-bar_2009,
	title = {The {F}-{BAR} protein {Syp}1 negatively regulates {WASp}-{Arp}2/3 complex activity during endocytic patch formation},
	volume = {19},
	issn = {0960-9822},
	url = {http://dx.doi.org/10.1016/j.cub.2009.10.062},
	doi = {10.1016/j.cub.2009.10.062},
	abstract = {BACKGROUND: Actin polymerization by Arp2/3 complex must be tightly
regulated to promote clathrin-mediated endocytosis. Although many Arp2/3
complex activators have been identified, mechanisms for its negative
regulation have remained more elusive. To address this, we analyzed the
yeast arp2-7 allele, which is biochemically unique in causing unregulated
actin assembly in vitro in the absence of Arp2/3 activators. RESULTS: We
examined endocytosis in arp2-7 mutants by live-cell imaging of Sla1-GFP, a
coat marker, and Abp1-RFP, which marks the later actin phase of
endocytosis. Sla1-GFP and Abp1-RFP lifetimes were accelerated in arp2-7
mutants, which is opposite to actin nucleation-impaired arp2 alleles or
deletions of Arp2/3 activators. We performed a screen for multicopy
suppressors of arp2-7 and identified SYP1, an FCHO1 homolog, which
contains F-BAR and AP-2micro homology domains. Overexpression of SYP1 in
arp2-7 cells slowed Sla1-GFP lifetimes closer to wild-type cells. Further,
purified Syp1 directly inhibited Las17/WASp stimulation of Arp2/3
complex-mediated actin assembly in vitro. This activity was mapped to a
fragment of Syp1 located between its F-BAR and AP-2micro homology domains
and depends on sequences in Las17/WASp outside of the VCA domain.
CONCLUSIONS: Together, these data identify Syp1 as a novel negative
regulator of WASp-Arp2/3 complex that helps choreograph the precise timing
of actin assembly during endocytosis.},
	number = {23},
	journal = {Curr. Biol.},
	author = {Boettner, Douglas R and D'Agostino, Jessica L and Torres, Onaidy Teresa and Daugherty-Clarke, Karen and Uygur, Aysu and Reider, Amanda and Wendland, Beverly and Lemmon, Sandra K and Goode, Bruce L},

@article{demel_relation_1975,
	title = {Relation between various phospholipase actions on human red cell membranes and the interfacial phospholipid pressure in monolayers},
	volume = {406},
	issn = {0006-3002},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/1174576},
	abstract = {The action of purified phospholipases on monomolecular films of various
interfacial pressures is compared with the action on erythrocyte
membranes. The phospholipases which cannot hyorolyse phospholipids of the
intact erythrocyte membrane, phospholipase C from Bacillus cereus,
phospholipase A2 from pig pancreas and Crotalus adamanteus and
phospholipase D from cabbage, can hydrolyse phospholipid monolayers at

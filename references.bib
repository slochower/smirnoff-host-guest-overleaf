	author = {Stanishneva-Konovalova, T B and Sokolova, O S},
	month = apr,
	year = {2015},
	keywords = {5)P, PPIs, PPIs - EndNote, actin cytoskeleton, bar, coarse-grained, coarse-grained model, domain, dppc, force-field, membrane curvature, molecular dynamics, phosphatidylinositol, phospholipid-bilayers, pi(4,5)p-2, proteins, systems},

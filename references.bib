	keywords = {\#nosource, Animals, Cell Separation, Chromatography, High Pressure Liquid, Chromatography, Ion Exchange, Chromatography, Thin Layer, Humans, Indicators and Reagents, Inositol, Inositol Phosphates, Lymphocyte Activation, Lymphocytes, Phosphatidylinositols, Radioactive Tracers, Signal Transduction, Staining and Labeling},

@article{patel_modeling_2017,
	title = {Modeling and simulation of bacterial outer membranes and interactions with membrane proteins},
	volume = {43},
	issn = {0959-440X},
	url = {http://dx.doi.org/10.1016/j.sbi.2017.01.003},
	doi = {10.1016/j.sbi.2017.01.003},
	abstract = {The outer membrane (OM) of Gram-negative bacteria is composed of
phospholipids in the periplasmic leaflet and lipopolysaccharides (LPS) in
the external leaflet, along with β-barrel OM proteins (OMPs) and lipidated
periplasmic lipoproteins. As a defensive barrier to toxic compounds, an

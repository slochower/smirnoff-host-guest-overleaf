@article{saunders_role_2006,
	title = {Role of vinculin in regulating focal adhesion turnover},
	volume = {85},
	issn = {0171-9335},
	url = {http://dx.doi.org/10.1016/j.ejcb.2006.01.014},
	doi = {10.1016/j.ejcb.2006.01.014},
	abstract = {Although vinculin (-/-) mouse embryo fibroblasts assemble focal adhesions
(FAs), they spread more slowly, less extensively, and close a wound more
rapidly than vinculin (+/+) cells. To investigate the structure and
dynamics of FAs in these cells, we used real-time interference reflection
microscopy (IRM) thus avoiding the need to express exogenous GFP-tagged FA
proteins which may be misregulated. This showed that the FAs were smaller,
less abundant and turned over more rapidly in vinculin null compared to
wild-type cells. Expression of vinculin rescued the spreading defect and
resulted in larger and more stable FAs. Phosphatidylinositol
4,5-bisphosphate (PIP2) is thought to play a role in vinculin activation
by relieving an intramolecular association between the vinculin head (Vh)
and tail (Vt) that masks the ligand binding sites in Vh and Vt. To
investigate the role of the vinculin/PIP2 interaction in FA dynamics, we
used a vinculin mutant lacking the C-terminal arm (residues 1053-1066) and
referred to as the deltaC mutation. This mutation reduced PIP2 binding to
a Vt deltaC polypeptide by {\textgreater}90\% compared to wild type without affecting
binding to Vh or F-actin. Interestingly, cells expressing the vinculin
deltaC mutant assembled remarkably stable FAs. The results suggest that
vinculin inhibits cell migration by stabilising FAs, and that binding of
inositol phospholipids to Vt plays an important role in FA turnover.},
	number = {6},
	journal = {Eur. J. Cell Biol.},
	author = {Saunders, Ruth M and Holt, Mark R and Jennings, Lisa and Sutton, Deborah H and Barsukov, Igor L and Bobkov, Andrey and Liddington, Robert C and Adamson, Eileen A and Dunn, Graham A and Critchley, David R},
	month = jun,
	year = {2006},
	keywords = {\#nosource, PPIs},
	pages = {487--500}
}


	keywords = {PPIs, PPIs - EndNote, cells, cholesterol, heterogeneity, localization, membrane domains, polarization, protein, rafts, signaling pathway, spermatocytes},
	pages = {1396--1407}
}

@article{akhtar_calcium_1978,

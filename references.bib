}

@article{wong_lamellar_2003,
	title = {Lamellar phase of stacked two-dimensional rafts of actin filaments},
	volume = {91},
	issn = {0031-9007},
	url = {http://dx.doi.org/10.1103/PhysRevLett.91.018103},
	doi = {10.1103/PhysRevLett.91.018103},
	abstract = {We examined liquid crystalline phases of the cytoskeletal polyelectrolyte
filamentous (F-)actin in the presence of multivalent counterions. As a
function of increasing ion concentration, the F-actin rods in either an
isotropic or a nematic phase will transform into a new and unexpected
lamellar phase of cross-linked rafts (L(XR) phase), before condensing into
a bundled phase of parallel, close-packed rods. This behavior is generic
for alkali earth divalent ions Mg2+, Ca2+, Sr2+, and Ba2+, and the
structural transitions are achieved without any architecture-specific
actin-binding linker proteins.},

@article{motlagh_ensemble_2014,
	title = {The ensemble nature of allostery},
	volume = {508},
	issn = {0028-0836},
	url = {http://dx.doi.org/10.1038/nature13001},
	doi = {10.1038/nature13001},
	abstract = {Allostery is the process by which biological macromolecules (mostly
proteins) transmit the effect of binding at one site to another, often
distal, functional site, allowing for regulation of activity. Recent
experimental observations demonstrating that allostery can be facilitated
by dynamic and intrinsically disordered proteins have resulted in a new
paradigm for understanding allosteric mechanisms, which focuses on the
conformational ensemble and the statistical nature of the interactions
responsible for the transmission of information. Analysis of allosteric
ensembles reveals a rich spectrum of regulatory strategies, as well as a
framework to unify the description of allosteric mechanisms from different

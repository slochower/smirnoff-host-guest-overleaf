@article{plimpton_fast_1995,
	title = {Fast {Parallel} {Algorithms} for {Short}-{Range} {Molecular} {Dynamics}},
	volume = {117},
	issn = {0021-9991},
	url = {http://dx.doi.org/10.1006/jcph.1995.1039},
	doi = {10.1006/jcph.1995.1039},
	abstract = {Three parallel algorithms for classical molecular dynamics are presented.
The first assigns each processor a fixed subset of atoms; the second
assigns each a fixed subset of inter-atomic forces to compute; the third
assigns each a fixed spatial region. The algorithms are suitable for
molecular dynamics models which can be difficult to parallelize
efficiently{\textbackslash}textemdashthose with short-range forces where the neighbors of
each atom change rapidly. They can be implemented on any
distributed-memory parallel machine which allows for message-passing of
data between independently executing processors. The algorithms are tested
on a standard Lennard-Jones benchmark problem for system sizes ranging
from 500 to 100,000,000 atoms on several parallel supercomputers--the
nCUBE 2, Intel iPSC/860 and Paragon, and Cray T3D. Comparing the results
to the fastest reported vectorized Cray Y-MP and C90 algorithm shows that
the current generation of parallel machines is competitive with
conventional vector supercomputers even for small problems. For large
problems, the spatial algorithm achieves parallel efficiencies of 90\% and
a 1840-node Intel Paragon performs up to 165 faster than a single Cray C9O
processor. Trade-offs between the three algorithms and guidelines for
adapting them to more complex molecular dynamics simulations are also
discussed.},
	number = {1},
	journal = {J. Comput. Phys.},
	author = {Plimpton, Steve},
	month = mar,
	year = {1995},
	keywords = {\#nosource, SMIRNOFF},
	pages = {1--19}
}


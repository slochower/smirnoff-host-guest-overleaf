	keywords = {*Molecular Dynamics Simulation, *Protons, *Quantum Theory, Calcium/chemistry, Cations, Divalent, Magnesium/chemistry, Models, Molecular, PPIs, PPIs - EndNote, Phosphatidylinositol 4,5-Diphosphate/*chemistry},
	pages = {8322--8329}
}


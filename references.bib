@article{chen_charge-reversal_2000,
	title = {Charge-reversal instability in mixed bilayer vesicles},
	volume = {62},
	issn = {1063-651X},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/11088740},
	abstract = {Bilayer vesicles form readily from mixtures of charged and neutral
surfactants. When such a mixed vesicle binds an oppositely charged object,
its membrane partially demixes: the adhesion zone recruits more charged
surfactants from the rest of the membrane. Given an unlimited supply of
adhering objects one might expect the vesicle to remain attractive until
it was completely covered. Contrary to this expectation, we show that a
vesicle can instead exhibit adhesion saturation, partitioning
spontaneously into an attractive zone with definite area fraction, and a
repulsive zone. The latter zone rejects additional incoming objects
because counterions on the interior of the vesicle migrate there,
effectively reversing the membrane's charge. The effect is strongest at
high surface charge densities, low ionic strength, and with thin,
impermeable membranes. Adhesion saturation in such a situation has
recently been observed experimentally [H. Aranda-Espinoza et al., Science
285, 394 (1999)].},
	number = {2 Pt B},
	journal = {Phys. Rev. E Stat. Phys. Plasmas Fluids Relat. Interdiscip. Topics},
	author = {Chen, Y and Nelson, P},
	month = aug,
	year = {2000},
	keywords = {\#nosource, PPIs, PPIs - EndNote},

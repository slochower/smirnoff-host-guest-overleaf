@article{eelkema_molecular_2006,
	title = {Molecular machines: nanomotor rotates microscale objects},
	volume = {440},
	issn = {0028-0836},
	url = {http://dx.doi.org/10.1038/440163a},
	doi = {10.1038/440163a},
	abstract = {Nanomachines of the future will require molecular-scale motors that can
perform work and collectively induce controlled motion of much larger
objects. We have designed a synthetic, light-driven molecular motor that
is embedded in a liquid-crystal film and can rotate objects placed on the
film that exceed the size of the motor molecule by a factor of 10,000. The
changes in shape of the motor during the rotary steps cause a remarkable
rotational reorganization of the liquid-crystal film and its surface
relief, which ultimately causes the rotation of submillimetre-sized
particles on the film.},

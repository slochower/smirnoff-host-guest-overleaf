@article{brannigan_contributions_2007,
	title = {Contributions of {Gaussian} curvature and nonconstant lipid volume to protein deformation of lipid bilayers},
	volume = {92},
	issn = {0006-3495},
	url = {http://dx.doi.org/10.1529/biophysj.106.094953},

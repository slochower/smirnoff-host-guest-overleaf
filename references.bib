	journal = {Structure},
	author = {Guan, Rong and Dai, Han and Han, Dai and Harrison, Stephen C and Kirchhausen, Tomas},
	month = sep,
	year = {2010},
	keywords = {\#nosource, PPIs},
	pages = {1191--1198}
}

